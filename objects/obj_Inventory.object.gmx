        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (showInv){
    draw_set_color(c_black);
    draw_set_alpha(0.8);
    draw_rectangle(x1,y1,x2,y2,0);
    draw_set_alpha(1);
    
    //draw inventory for pickups
    for (i = 0; i &lt; 4; i++){
        for (j = 0; j &lt; 4; j++){
            draw_sprite_ext(spr_Border,0,x1 + initOff + (i * initSpace), y1 + initOff + (j * initSpace),2,2,0,c_white,1);
            if (global.inventory[i,j] != -1){
                draw_sprite_ext(global.inventory[i,j].sprite_index, 0, x1 + initOff + (i * initSpace), y1 + initOff + (j * initSpace),2,2,0,c_white,1);
            }
        }
    }
    
    //draw inventory for equippable items
    for (i = 0; i &lt; 4; i++){
        draw_sprite_ext(spr_Border,0,midx, midy + initOff + (i * initSpace),2,2,0,c_white,1);
    }
    draw_sprite_ext(spr_Border,0,midx + initSpace, midy + initOff + initSpace,2,2,0,c_white,1);
    draw_sprite_ext(spr_Border,0,midx - initSpace, midy + initOff + initSpace,2,2,0,c_white,1);
    for (i = 0; i &lt; 6; i++){
        if(global.equippedItems[i] != -1){
            if(i = equipSlots.HEAD){
                draw_sprite_ext(global.equippedItems[i].sprite_index, 0, midx, midy + initOff + (0 * initSpace),2,2,0,c_white,1);
            }
            if(i = equipSlots.CHEST){
                draw_sprite_ext(global.equippedItems[i].sprite_index, 0, midx, midy + initOff + (1 * initSpace),2,2,0,c_white,1);
            }
            if(i = equipSlots.LEGS){
                draw_sprite_ext(global.equippedItems[i].sprite_index, 0, midx, midy + initOff + (2 * initSpace),2,2,0,c_white,1);
            }
            if(i = equipSlots.FEET){
                draw_sprite_ext(global.equippedItems[i].sprite_index, 0, midx, midy + initOff + (3 * initSpace),2,2,0,c_white,1);
            }
            if(i = equipSlots.WEAPON){
                draw_sprite_ext(global.equippedItems[i].sprite_index, 0, midx + initSpace, midy + initOff + (1 * initSpace),2,2,0,c_white,1);
            }
            if(i = equipSlots.GLOVES){
                draw_sprite_ext(global.equippedItems[i].sprite_index, 0, midx - initSpace, midy + initOff + (1 * initSpace),2,2,0,c_white,1);
            } 
        }
    }
    
    if (global.objectHeld != -1){
        draw_sprite_ext(global.objectHeld.sprite_index, 0, mouse_x, mouse_y,2,2,0,c_white,1);
    }
    
    if(displayItemInfo){
        if(itemToDisplay.itemType == equipSlots.WEAPON){
            draw_set_color(c_black);
            draw_rectangle(mouse_x, mouse_y, mouse_x + 200, mouse_y + 50,0);
            draw_set_color(c_white);
            draw_text(mouse_x + 10, mouse_y, "Damage: " + string(itemToDisplay.damage));
            if (itemToDisplay.fireRate == 0){
                draw_text(mouse_x + 10, mouse_y + 20, "Fire Rate: Semi Auto" );
            }else{
                draw_text(mouse_x + 10, mouse_y + 20, "Fire Rate: " + string(itemToDisplay.fireRate) + " per second");
            }
            
        }
        if(itemToDisplay.itemType == equipSlots.HEAD){
            draw_set_color(c_black);
            draw_rectangle(mouse_x, mouse_y, mouse_x + 200, mouse_y + 50,0);
            draw_set_color(c_white);
            draw_text(mouse_x + 10, mouse_y, "Health: " + string(itemToDisplay.health_bonus));
        }
    }
    draw_set_color(c_black);
    
}
